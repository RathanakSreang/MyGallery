<center><h3>New Gallery</h3></center>
<div class="row">
  <div class="col-md-4 col-md-offset-4 well">
    <%= form_for @gallery do |f| %>
      <div class="form-group">
        <%= f.label :title %>
        <%= f.text_field :title, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.submit "Create", class: "btn btn-sm btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
