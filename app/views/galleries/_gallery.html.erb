<div class="col-lg-3 col-md-4 col-xs-6 thumb">
  <div data-si-mousemove-trigger="100"  class="shuffle-me offset__1">
    <%= gallery.title %>
    <%= link_to gallery do %>
      <div class="images">
        <%= image_tag gallery.photos.first.path.url, class: "img-responsive" %>
      </div>      
    <% end %>
  </div>
</div>
